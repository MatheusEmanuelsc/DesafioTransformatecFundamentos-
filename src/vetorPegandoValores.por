programa
{
	
	funcao inicio()
	{
		inteiro num[5],ind
		

		para(ind=0;ind<5;ind ++){
			escreva("Informe indice[",ind,"] : ",num[ind])	
			leia (num[ind])
			limpa()
			}
		para(ind=0;ind<5;ind ++){
			escreva("indice",ind,"e: ",num[ind],"\n")
			}
	}
}
